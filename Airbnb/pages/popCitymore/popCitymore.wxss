/* pages/popCitymore/popCitymore.wxss */
.all{
  background-color: #F1F1F1;
}
.title{
  height:130rpx;
  width: 100%;
  font-size: 32rpx;
  position: fixed;
  top: 0;
  background-color: white;
}
.return{
  position: absolute;
  top: 36%;

  left: 40rpx;
}
.collect{
  line-height: 140rpx;
  text-align: center;
  width: 100%;
}
.cover{
  padding-top: 60rpx;
  height: 60rpx;
}
.top{
  width: 100%;
  height: 300rpx;
  overflow: hidden;
  text-align: center;
}
.top image{
  width: 100%;
  height: 500rpx;
}
.top-title{
  position: absolute;
  top: 200rpx;
  left: 130rpx;
  font-size: 60rpx;
  color: white;
  font-weight: bold;
}
.top-introduce{
  position: absolute;
  top: 280rpx;
  left: 130rpx;
  font-size: 32rpx;
  color: white;
}
.content{
  width: 700rpx;
  margin: 50rpx 30rpx;
}
.content-title{
  color: #484848;
  font-size: 50rpx;
  font-weight: bold;
}
.content-introduce{
  color: #484848;
  font-size: 32rpx;
  padding:20rpx 0rpx;
}
.topRoom{
  width: 100%;
  border: 1px solid #F1F1F1;
  border-radius: 20rpx;
  box-sizing:border-box;
  margin-top:40rpx;
  background-color: white;
  position: relative;
}
.topRoom .swiper{
  width: 100%;
  height: 400rpx;
}
.topRoom .popRoomImgs{
  width: 100%;
  height: 360rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.popRoomTop{
  width: 170rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-weight: bold;
  background-color: #EB5257;
  color: white;
  text-align: center;
  border-radius: 10rpx;
  position: absolute;
  top: 20rpx;
  left: 30rpx;
}
.popRoomLandlord{
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx;
  border: 6rpx solid #fff;
  position: absolute;
  right: 30rpx;
  bottom:190rpx;
}
.popRoomKeep{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 20rpx;
  right:30rpx;
}
.popRoomTitle{
  padding: 20rpx 20rpx 0rpx 20rpx;
  font-size: 28rpx;
  color: #484848;
}
.popRoomIntroduce{
  padding:0rpx 26rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #484848;
  padding-bottom: 20rpx;
}
.popRoomBottom{
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 120rpx;
  border-top: 2px solid #F1F1F1;
  padding: 36rpx 20rpx;
  box-sizing: border-box;
  vertical-align: middle;
}
.popRoomPrice{
  height: 50rpx;
  line-height: 50rpx;
  color: #484848;

}
.popRoomEvaluation{
  color: #484848;
  display: flex;
  vertical-align: middle;
}
.popRoomEvaluation image{
  width: 150rpx;
  height: 50rpx;
}
.popRoomEvaluationNumber{
  font-size: 28rpx;
  height: 50rpx;
  line-height: 50rpx;
}